<!DOCTYPE html>
<html class="theme-light" lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta property="og:site_name" content="GoAnimate">
		<title>Make a Character | GoAnimate</title>
		<meta name="description" content="Make a video online for free with GoAnimate! Make videos for YouTube, create educational videos, make animated e-cards and more!">

		<link href="/static/go/css/site.css" rel="stylesheet" type="text/css">
		<link href="/static/go/css/style.css" rel="stylesheet" type="text/css">
		<link href="/static/go/css/site_responsive.css" rel="stylesheet" type="text/css">
		<link href="/static/go/css/app/cc.css" rel="stylesheet" type="text/css">

		<script src="/static/go/js/libs/jquery/jquery-1.11.0.min.js"></script>
		<script src="/static/go/js/libs/jquery/jquery.swfobject.min.js"></script>
		<script src="/static/go/js/libs/Gettext.js"></script>
		<script src="/static/go/po/goserver_js-en_US.js"></script>
		<script> // set up gettext
			var I18N_LANG = "en_US";
			var GT = new Gettext({
				"locale_data": json_locale_data
			});
		</script>
	</head>
	<body>
		<%- include("../includes/header", { user: user }); %>

		<section class="page-breadcrumb">
			<div class="container flex-container">
				<a href="/">Home</a>
				<div class="divider">></div>
				<a href="/themelist">Themes</a>
				<div class="divider">></div>
				<a href="/go/character_creator/<%= themeId %>"><%= themelist.find(v => v.cc_theme_id == themeId && v.features.cc)?.name || "Comedy World" %> Characters</a>
				<div class="divider">></div>
				<a href="#">New Character</a>
			</div>
		</section>

		<main>
			<div class="container flex-container centered">
				<div id="char_holder"></div>
			</div>
		</main>

		<%- include("../includes/footer", {}); %>

		<script>
			function characterSaved() {
				window.location.href = "/go/character_creator/<%= themeId %>";
			}

			var studio_data = {
				id: "Studio",
				swf: "/static/animation/414827163ad4eb60/cc.swf",
				width: "100%",
				height: "100%",

				align: "middle",
				allowScriptAccess: "always",
				allowFullScreen: "true",
				wmode: "window",

				hasVersion: "10.3"
			};

			studio_data.flashvars = {
				"original_asset_id": "<%= charId %>",
				"apiserver": "\/",
				"storePath": "\/static\/store\/3a981f5cb2739137\/<store>",
				"clientThemePath": "\/static\/ad44370a650793d9\/<client_theme>",
				"ut": 60,
				"lid": "0",
				"ctc": "go",
				"tlang": "en_US",
				"siteId": "13",
				"appCode": "go",
				"lang": "en",
				"uisa": 0,
				"themeId": "<%= themeId %>",
				"bs": "<%= bs %>"
			};
			$("#char_holder").flash(studio_data);
		</script>
		<script>
			function locStorage(action, key, value) {
				var result;
				if (typeof (localStorage) == 'undefined') {
					// localStorage not supported
				} else {
					try {
						switch (action) {
							case 'setItem':
								localStorage.setItem(key, value);
								break;
							case 'getItem':
								result = localStorage.getItem(key);
								break;
							case 'removeItem':
								localStorage.removeItem(key);
								break;
						}
						return result;
					} catch (e) {
						if (e == QUOTA_EXCEEDED_ERR) {
							//data wasn't successfully saved due to quota exceed so throw an error
						}
					}
				}
			}
		</script>
	</body>
	<!-- GRA JS -->
	<script src="/static/go/js/go2.js"></script>
	<script src="/static/go/js/site.js"></script>
</html>
